@{
    var Register = ViewData["Register"] as List<Web_congty.Modal.FW.Information_company>;
}
@section Scripts{
    <script src="~/Public/Admin/js/Register_c.js"></script>
    <script src="~/Public/Admin/plugins/ckfinder/ckfinder.js"></script>
}
<div class="QA_warapper-content-category">
    <div class="col-md-12">
        <div class="row">
            <div class="col-md-12">
                <div class="header-content-category">
                    <div class="menu-crud float-lg-right mt-2">
                        <ul>
                            <li class="d-inline-block mr-2">
                                <a class="btn btn-xs btn-orange " href="#"><i class="fas fa-recycle mr-1"></i> Tải lại</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="card-body ">
                <div class="col-md-12">
                    <div class="content-category">
                        @foreach (var item in Register)
                        {
                        <form class="row mauchu" action="">
                            <div class="form-group  col-3">
                                <label for="Name_CPN">Đăng ký công ty</label>
                                <input type="text" class="form-control" id="Name_CPN" value="@item.Name_CPN">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Phone">Số điện thoại</label>
                                <input type="text" class="form-control" id="Phone" value="@item.Phone">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Phone">Hot line</label>
                                <input type="text" class="form-control" id="Phone_hot" value="@item.Phone_hot">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Phone">Ngân hàng</label>
                                <input type="number" class="form-control" id="Bank_number" value="@item.Bank_number">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Phone">Email_Khác</label>
                                <input type="text" class="form-control" id="Email_true" value="@item.Email_true">
                            </div>
                            <div class="form-group col-3">
                                <label for="Email">Email</label>
                                <input type="text" class="form-control " id="Email" value="@item.Email">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Facebook">Facebook</label>
                                <input type="text" class="form-control" id="Facebook" value="@item.Facebook">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Email_CPN">Email hỗ trợ</label>
                                <input type="text" class="form-control" id="Email_CPN" value="@item.Email_CPN">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Youtube">Youtube</label>
                                <input type="text" class="form-control" id="Youtube" value="@item.Youtube">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Google">Google</label>
                                <input type="text" class="form-control" id="Google" value="@item.Google">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Viber">Viber</label>
                                <input type="text" class="form-control" id="Viber" value="@item.Viber">
                            </div>

                            <div class="form-group  col-3">
                                <label for="Slogan">Mô tả ngắn</label>
                                <input type="text" class="form-control" id="Slogan" value="@item.Slogan">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Addred">Địa chỉ</label>
                                <input type="text" class="form-control" id="Addred" value="@item.Addred">
                            </div>
                            <div class="form-group  col-3">
                                <label>Logo</label>
                                <p><a class="btn btn-primary btn-sm d-block" onclick="BrowseServer_Logo()">Chọn Logo</a></p>
                                <input type="hidden" id="Logo" value="@item.Logo" />
                                <p id="show_logo" class="m-0"><img src="@item.Logo" style="width:40px;height:30px;" /></p>
                            </div>
                            <div class="form-group  col-3">
                                <label>Logo</label>
                                <p><a class="btn btn-primary btn-sm d-block" onclick="BrowseServer_Advertisement_QC()">Chọn Logo</a></p>
                                <input type="hidden" id="Advertisement_QC" value="@item.Advertisement_QC" />
                                <p id="show_Advertisement_QC" class="m-0"><img src="@item.Advertisement_QC" style="width:40px;height:30px;" /></p>
                            </div>
                            <div class="form-group  col-3">
                                <label for="Register">NGày Đăng ký</label>
                                @if (item.Register == null)
                                {
                                    <input type="date" class="form-control" id="Register" value="@item.Register">
                                }
                                else
                                {
                                    <input type="date" class="form-control" id="Register" value="@item.Register.GetValueOrDefault().ToString("yyyy-MM-dd")">
                                }
                            </div>
                            <div class="form-group  col-3">
                                <label for="Notification_TB">THông báo</label>
                                <input type="text" class="form-control" id="Notification_TB" value="@item.Notification_TB">
                            </div>
                            <div class="form-group  col-3">
                                <label for="Best_Company">Slogan </label>
                                <input type="text" class="form-control" id="Best_Company" value="@item.Best_Company">
                            </div>
                        </form>
                        }
                    </div>
                </div>
                <div class="bottom_save_setting">
                    <a class="btn btn-xs btn-success " id="Save_Register" href="#"><i class="fas fa-check-circle mr-2"></i>Cập nhật</a>
                </div>
            </div>
        </div>
    </div>
</div><!--/QA_warapper-content-category-->
