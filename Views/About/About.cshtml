
@{
    ViewBag.Title = "Về chúng tôi";
}

@section scripts{
    <script src="~/Public/Client/js/Map_vechungtoi.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCWYmbl0boe3qZcg98VZ_4CemQ1cE_oqs4&callback=initMap"></script>
