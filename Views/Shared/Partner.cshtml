@{
    var list_partner = ViewData["list_partner"] as List<Web_congty.Modal.FW.Partner_DT>;
}
<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="clients-carousel owl-carousel">
                @{
                    foreach (var item in list_partner)
                    {
                        <div class="client-logo-wrapper d-table">
                            <div class="client-logo d-table-cell">
                                <img src="@item.image_partner" alt="@item.Name_partner">
                            </div>
                        </div>
                    }
                }
            </div>
        </div>
    </div>
</div>
