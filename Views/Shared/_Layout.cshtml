<!doctype html>
<html lang="vi">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <!-- Mobile Specific Metas ================================-->
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Site Title- -->
    <title>@ViewBag.title</title>
    <!-- CSS
    ==================================================== -->
    <!--Font Awesome -->
    <link href="~/public/Client/css/font-awesome.min.css" rel="stylesheet" />
    <!--Animate CSS -->
    <link href="~/public/Client/css/animate.css" rel="stylesheet" />
    <!-- Iconic Fonts -->
    <link href="~/public/Client/css/icofonts.css" rel="stylesheet" />
    <!-- Bootstrap CSS -->
    <link href="~/public/Client/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Owl Carousel -->
    <link href="~/public/Client/css/owlcarousel.min.css" rel="stylesheet" />
    <!-- Video Popup -->
    <link href="~/public/Client/css/magnific-popup.css" rel="stylesheet" />
    <!--Style CSS -->
    <link href="~/public/Client/css/style.css" rel="stylesheet" />
    <!--Responsive CSS -->
    <link href="~/public/Client/css/responsive.css" rel="stylesheet" />
    <link href="~/Content/toastr.css" rel="stylesheet" />
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
       <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
       <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
       <![endif]-->

</head>

<body>
    <div class="search-bar">
        <i class="fa fa-close"></i>
        <form class="search-bar-fixed" action="#">
            <input type="text" placeholder="Tìm kiếm">
            <button type="submit"><i class="fa fa-search"></i></button>
        </form>
    </div>
    <!--End Search bar -->
    @{ Html.RenderAction("Top_Header", "Home");}
    <!-- nếu tên của action không trùng với tên của view cshtml thì phải return PartialView("tên file trùng với file.cshtml") -->
    <!-- header======================-->
    <header>
        @{ Html.RenderAction("Header", "Home");}
    </header>
    <!-- End of Header area=-->
    @RenderBody()


    <section id="tw-client" class="tw-client">
        @{ Html.RenderAction("Partner", "Home");}
    </section>
    <footer id="tw-footer" class="tw-footer">
        @{ Html.RenderAction("Footer", "Home");}
    </footer>
    <!-- Javascripts File
       =============================================================================-->
    <!-- initialize jQuery Library -->
    <script src="~/public/Client/js/jquery-2.0.0.min.js"></script>
    <!-- Popper JS -->
    <script src="~/public/Client/js/popper.min.js"></script>
    <!-- Bootstrap jQuery -->
    <script src="~/public/Client/js/bootstrap.min.js"></script>
    <!-- Owl Carousel -->
    <script src="~/public/Client/js/owl-carousel.2.3.0.min.js"></script>
    <!-- Waypoint -->
    <script src="~/public/Client/js/waypoints.min.js"></script>
    <!-- Counter Up -->
    <script src="~/public/Client/js/jquery.counterup.min.js"></script>
    <!-- Video Popup -->
    <script src="~/public/Client/js/jquery.magnific.popup.js"></script>
    <!-- WOW JS -->
    <script src="~/public/Client/js/wow.min.js"></script>
    <!-- SmoothScroll JS -->
    @*<script src="~/Public/Client/js/smoothscroll.js"></script>*@
    <!-- Template Custom -->
    <script src="~/public/Client/js/main.js"></script>
    <script src="~/Public/Client/js/home/home.js"></script>
    <script src="~/Scripts/toastr.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Public/Client/js/dangkynhanbaiviet.js"></script>
    <script>
        $(document).ready(function () {
            $("#success-alert").fadeTo(2000, 1000).slideUp(1000, function () {
                $("#success-alert").slideUp(1000);
            });
            $("#success-alert_dangnhap").fadeTo(1500, 500).slideUp(1150, function () {
                $("#success-alert").slideUp(500);
                $("#myModal_Login").modal("show");
            });
            $('.owl-carousel').owlCarousel({
                dots: false,
            });
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>